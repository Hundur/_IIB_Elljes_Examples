BROKER SCHEMA Trim_String_Padding



CREATE COMPUTE MODULE Trim_String_Padding
	CREATE FUNCTION Main() RETURNS BOOLEAN
	BEGIN
		
		SET OutputRoot = InputRoot;
		
		DECLARE ValueToTrim CHARACTER;

		SET ValueToTrim = InputRoot.XMLNSC.GuestBook.Guest.FName;
		
		SET ValueToTrim = TRIM(LEADING '0' FROM ValueToTrim);
			
		SET OutputRoot.XMLNSC.GuestBook.Guest.FName = ValueToTrim;		
		
		RETURN TRUE;
	END;
END MODULE;
